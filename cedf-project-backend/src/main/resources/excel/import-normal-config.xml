<?xml version="1.0" encoding="ISO-8859-1"?>
    <workbook>
        <worksheet idx="0">
            <section startRow="0" endRow="2" />
            <loop startRow="3" endRow="3" items="projects" var="pjt" varType="io.nakong.modules.project.vo.ProjectNormalVo">
                <section startRow="3" endRow="3">
                    <mapping row="3" col="0">pjt.donatorName                            </mapping> 
                    <mapping row="3" col="1">pjt.cashAmount                          </mapping>
                    <mapping row="3" col="2">pjt.donateDate            </mapping> 
                    <mapping row="3" col="3">pjt.withAmount          </mapping> 
                    <mapping row="3" col="4">pjt.withDate            </mapping> 
                </section>
                <loopbreakcondition>
                    <rowcheck offset="0">
                        <cellcheck offset="0"></cellcheck>
                    </rowcheck>
                </loopbreakcondition>
            </loop>
        </worksheet>
    </workbook>